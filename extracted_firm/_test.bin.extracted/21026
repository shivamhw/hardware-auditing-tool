}
function start() 
{ 
wtime--;
if (wtime >= 0)
{ 
document.wlanWait.time.value = wtime;
id=setTimeout("start()",1000);
}
if (wtime == 0)
{ 
document.wlanWait.submit();
return true;
}
}
