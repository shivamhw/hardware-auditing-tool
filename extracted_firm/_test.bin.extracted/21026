<title>ADSL Settings </title>
<script language="javascript"></script>
<SCRIPT>
function dhcpTblClick(url) {
openWindow(url, 'DHCPTbl' );
}
function adsltoneClick(url)
{
var wide=600;
var high=400;
if (document.all)
var xMax = screen.width, yMax = screen.height;
else if (document.layers)
var xMax = window.outerWidth, yMax = window.outerHeight;
else
var xMax = 640, yMax=480;
var xOffset = (xMax - wide)/2;
var yOffset = (yMax - high)/3;
var settings = 'width='+wide+',height='+high+',screenX='+xOffset+',screenY='+yOffset+',top='+yOffset+',left='+xOffset+', resizable=yes, toolbar=no,location=no,directories=no,status=no,menubar=no,scrollbars=yes';
window.open( url, 'ADSLTONETbl', settings );
}
function updateStatus()
{
if( document.set_adsl.adsl2.checked == true){
document.set_adsl.anxl.disabled = false;
}else{
document.set_adsl.anxl.checked = false;
document.set_adsl.anxl.disabled = true;
}
if( document.set_adsl.adsl2.checked == true || document.set_adsl.adsl2p.checked == true){
document.set_adsl.anxm.disabled = false;
document.set_adsl.sra.disabled = false;
}else{
document.set_adsl.anxm.checked = false;
document.set_adsl.anxm.disabled = true;
document.set_adsl.sra.checked = false;
document.set_adsl.sra.disabled = true;
}
}
</SCRIPT>
