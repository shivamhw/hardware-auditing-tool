&*#@upload.htm@#*&2651
<VWS_FUNCTION (void*)SendWebMetaStr();>
<VWS_FUNCTION (void*)SendWebCssStr();>
<title>Firmware Update</title>
<script>
var uploadClick=0;
function sendClicked()
{
var str =  document.getElementById("uploadfilename").value;
if(str.length==0)	{
alert("Please select file to upload");
return false;
}
if (!confirm('Do you really want to upgrade the firmware?'))
return false;
else
{
if(uploadClick==0)
{
uploadClick=1;
}
else
{
alert("The request is in process, please wait a few seconds!");
return false;
}
return true;
}
}
</script>
<link href="stylemain.css" rel="stylesheet" type="text/css">
</head>
<BODY>
<blockquote>
<script language="JavaScript">
TabHeader="Maintenance";
SideItem="Firmware Update";
HelpItem="firmwareupdate";
</script>
<script type='text/javascript'>
mainTableStart();
logo();
TopNav();
ThirdRowStart();
Write_Item_Images();
mainBodyStart();
</script>
<form action="form2file.htm" method=POST enctype="multipart/form-data" name="password">
