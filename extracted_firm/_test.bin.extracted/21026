var wtime;
<VWS_FUNCTION (void*)rebootwaittime();>
function stop()
{
clearTimeout(id); 
}
function start() 
{ 
wtime--;
if (wtime >= 0)
{ 
frm.time.value = wtime;
id=setTimeout("start()",1000);
}
if (wtime == 0)
{ 
window.open( 
<VWS_FUNCTION (void*)httpMgmtIPAddrUrl();>
,target="_parent");
}
}
