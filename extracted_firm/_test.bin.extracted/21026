var uploadClick=0;
function resetClick()
{
if ( !confirm('Do you really want to reset the current settings to default?') ) {
return false;
}
else
return true;
}
function sendClicked()
{
if (!confirm('Do you really want to upgrade the settings?'))
return false;
else
{
if(uploadClick==0)
{
uploadClick=1;
}
else
{
alert("The request is in process, please wait a few seconds!");
return false;
}
return true;
}
}
