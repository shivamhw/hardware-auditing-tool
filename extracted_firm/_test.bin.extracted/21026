var uploadClick=0;
function sendClicked()
{
var str = document.getElementById("uploadfilename").value;
if(str.length==0) {
alert("Please select config file to update");
return false;
}
if (!confirm('Do you really want to update the settings?'))
return false;
else
{
if(uploadClick==0)
{
uploadClick=1;
}
else
{
alert("The request is in process, please wait a few seconds!");
return false;
}
return true;
}
}
