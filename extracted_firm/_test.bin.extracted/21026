&*#@oamloopback.htm@#*&3996
<VWS_FUNCTION (void*)SendWebMetaStr();>
<VWS_FUNCTION (void*)SendWebCssStr();>
<title>OAM Loopback Test </title>
<SCRIPT>
var oamGoClick=0;
function isHexDecimal(num)
{
var string="1234567890ABCDEF";
if (string.indexOf(num.toUpperCase()) != -1)
{
return true;
}
return false;
}
function isValidID(val)
{
for(var i=0; i < val.length; i++)
{       
if  ((!isHexDecimal(val.charAt(i))))
{
return false;
}
}
return true;
}
function goClick()
{
digit = parseInt(document.oamlb.oam_vpi.value, 10);
if ( (document.oamlb.oam_vpi.value == "") ||
validateKey(document.oamlb.oam_vpi.value) == 0 ||
(digit > 255 || digit < 0) ) {
alert("Invalid VPI value! You should set a value between 0-255.");
document.oamlb.oam_vpi.focus();
return false;
}
if(document.oamlb.oam_flow[0].checked ==  true  || document.oamlb.oam_flow[1].checked==true){
digit = parseInt(document.oamlb.oam_vci.value, 10);
if ( (document.oamlb.oam_vci.value == "") ||
validateKey(document.oamlb.oam_vci.value) == 0 ||
(digit > 65535 || digit < 32) ) {
alert("Invalid VCI value! You should set a value between 32-65535.");
document.oamlb.oam_vci.focus();
return false;
}
}
retval = isValidID(document.oamlb.oam_llid.value);
if((document.oamlb.oam_llid.value=="")|| (retval==false))
{ 
alert("Invalid Loopback Location ID!")
document.oamlb.oam_llid.focus()
return false
}
if(oamGoClick==0)
{
oamGoClick=1;
}
else
{
alert("The request is in process, please wait a few seconds!");
return false;
}
return true;
}
function FlowCheck()
{
if(document.oamlb.oam_flow[0].checked ==  true  || document.oamlb.oam_flow[1].checked==true)
{
document.oamlb.oam_vci.value="";  	
if(document.all.showvci)
document.all.showvci.style.display = "block"
}else if (document.oamlb.oam_flow[2].checked==true)
{
document.oamlb.oam_vci.value="3";
if(document.all.showvci)
document.all.showvci.style.display = "none"
}else if (document.oamlb.oam_flow[3].checked==true)
{
document.oamlb.oam_vci.value="4";
if(document.all.showvci)
document.all.showvci.style.display = "none"
}
return true;
}
</SCRIPT>
</head>
<body>
